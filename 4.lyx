\begin_layout Section
Sea 
\begin_inset Formula $f(x,y)=\cos(xy).$
\end_inset

 Además 
\begin_inset Formula $x$
\end_inset

 e 
\begin_inset Formula $y$
\end_inset

 son funciones de las variables 
\begin_inset Formula $u$
\end_inset

 y 
\begin_inset Formula $v$
\end_inset

 de acuerdo a las siguientes fórmulas: 
\begin_inset Formula $x(u,v)=u+v,y(u,v)=u-v$
\end_inset

.
 Calcular
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial u^{2}}\left(x\left(u,v),y\left(u,v\right)\right)\right)\mbox{ y }\frac{\partial^{3}f}{\partial u\partial v^{2}}\left(x\left(u,v\right),y\left(u,v\right)\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Sustituyendo
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f(x(u,v),y(u,v))=\cos\left(\left(u+v\right)\left(u-v\right)\right)=\cos\left(u^{2}-v^{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial u}=-2u\sin\left(u^{2}-v^{2}\right);\frac{\partial^{2}f}{\partial u^{2}}=-2\sin\left(u^{2}-v^{2}\right)-4u^{4}\cos\left(u^{2}-v^{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial v}=2v\sin\left(u^{2}-v^{2}\right);\frac{\partial f}{\partial v^{2}}=2\sin\left(u^{2}-v^{2}\right)-4v^{2}\cos\left(u^{2}-v^{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{3}f}{\partial u\partial v^{2}}\left(x\left(u,v\right),y\left(u,v\right)\right)=4u^{2}\cos\left(u^{2}-v^{2}\right)+4v^{2}2u\sin\left(u^{2}-v^{2}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Usando la regla de la cadena
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\nabla f(x,y)=\left(-y\sin xy,-x\sin xy\right);Hf(x,y)=\begin{matrix}-y^{2}\cos xy & -yx\cos xy\\
-yx\cos xy & -x\cos xy
\end{matrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\nabla x(u,v)=\left(1,1\right);\nabla y(u,v)=\left(1,-1\right)\Rightarrow Hx(u,v)=Hy(u,v)=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Sea }g(u,v)=(x(u,v),y(u,v))=(u+v,u-v)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore D\left(f\circ g\right)(u,v)=D(f(x(u,v),y(u,v)))*D(g(u,v))=\left(-y\sin xy,-x\sin xy\right)*\left(\begin{matrix}1 & 1\\
1 & -1
\end{matrix}\right)=\left(-y\sin xy-x\sin xy,-y\sin xy+x\sin xy\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(\sin\left(u^{2}-v^{2}\right)\left(-u-v-u+v\right),\sin\left(u^{2}-v^{2}\right)\left(u+v-u+v\right)\right)=\left(\sin\left(u^{2}-v^{2}\right)\left(-u-v-u+v\right),\sin\left(u^{2}-v^{2}\right)\left(u+v-u+v\right)\right)\rightarrow\mbox{Reemplacé}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Rightarrow D\left(f\circ g\right)(u,v)=\left(\sin\left(u^{2}-v^{2}\right)\left(-2u\right),\sin\left(u^{2}-v^{2}\right)\left(2v\right)\right)\rightarrow\mbox{Aquí seguiría operando como con una función normal}
\]

\end_inset


\end_layout

\begin_layout Section
Se dice que una función 
\begin_inset Formula $f:\mathbb{R}^{n}\rightarrow\mathbb{R}$
\end_inset

 de clase 
\begin_inset Formula $C^{2}$
\end_inset

 satisface la ecuación de Laplace o bien que es una función armónica en
 un conjunto abierto 
\begin_inset Formula $U\subset\mathbb{R}^{3}$
\end_inset

 si:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\nabla f=\frac{\partial^{2}f}{\partial x_{1}^{2}}+\ldots+\frac{\partial^{2}f}{\partial x_{n}^{2}}=\nabla^{2}f\equiv0\mbox{ en }U
\]

\end_inset


\end_layout

\begin_layout Paragraph
Verificar que las siguientes funciones son armónicas en un conjunto abierto
 
\begin_inset Formula $U\subset\mathbb{R}^{3}$
\end_inset

.
 Determinar 
\begin_inset Formula $U$
\end_inset

 en cada caso:
\end_layout

\begin_layout Subsection
\begin_inset Formula $f\left(x,y,z\right)=x^{2}+y^{2}-2z^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial x}\left(x,y,z\right)=2x\rightarrow\frac{\partial^{2}f}{\partial x^{2}}\left(x,y,z\right)=2
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial y}\left(x,y,z\right)=2y\rightarrow\frac{\partial^{2}f}{\partial y^{2}}\left(x,y,z\right)=2
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial z}\left(x,y,z\right)=-4z\rightarrow\frac{\partial^{2}f}{\partial z^{2}}\left(x,y,z\right)=-4
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\frac{\partial^{2}f}{\partial x^{2}}\left(x,y,z\right)+\frac{\partial^{2}f}{\partial y^{2}}\left(x,y,z\right)+\frac{\partial^{2}f}{\partial z^{2}}\left(x,y,z\right)=2+2-4=0\rightarrow\mbox{Es laplaciano para todo }U\subset\mathbb{R}^{3}
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f\left(x,y,z\right)=\ln\sqrt{x^{2}+y^{2}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial x}\left(x,y,z\right)=\frac{1}{\sqrt{x^{2}+y^{2}}}*\frac{x}{\sqrt{x^{2}+y^{2}}}=\frac{x}{x^{2}+y^{2}}\rightarrow\frac{\partial^{2}f}{\partial x^{2}}\left(x,y,z\right)=\frac{1}{x^{2}+y^{2}}-\frac{2x}{\left(x^{2}+y^{2}\right)^{2}}=\frac{x^{2}+y^{2}-2x^{2}}{\left(x^{2}+y^{2}\right)^{2}}=\frac{y^{2}-x^{2}}{\left(x^{2}+y^{2}\right)^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial y}\left(x,y,z\right)=\frac{y}{x^{2}+y^{2}}\rightarrow\frac{\partial^{2}f}{\partial y^{2}}\left(x,y,z\right)=\frac{x^{2}-y^{2}}{\left(x^{2}+y^{2}\right)^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial z^{2}}\left(x,y,z\right)=0\rightarrow\frac{\partial^{2}f}{\partial z^{2}}\left(x,y,z\right)=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore\frac{\partial^{2}f}{\partial x^{2}}\left(x,y,z\right)+\frac{\partial^{2}f}{\partial y^{2}}\left(x,y,z\right)+\frac{\partial^{2}f}{\partial z^{2}}\left(x,y,z\right)=\frac{y^{2}-x^{2}}{\left(x^{2}+y^{2}\right)^{2}}+\frac{x^{2}-y^{2}}{\left(x^{2}+y^{2}\right)^{2}}=0\rightarrow\mbox{Es laplaciano para todo }U\subset\mathbb{R}^{3}
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x,y,z)=\frac{1}{\sqrt{x^{2}+y^{2}+z^{2}}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial x}\left(x,y,z\right)=-x\left(x^{2}+y^{2}+z^{2}\right)^{-3/2}\rightarrow\frac{\partial^{2}f}{\partial x^{2}}\left(x,y,z\right)=-\left(x^{2}+y^{2}+z^{2}\right)^{-3/2}-3x^{2}\left(x^{2}+y^{2}+z^{2}\right)^{-5/2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{-x^{2}-y^{2}-z^{2}+3x^{2}}{\left(x^{2}+y^{2}+z^{2}\right)^{5/2}}=\frac{2x^{2}-y^{2}-z^{2}}{\left(x^{2}+y^{2}+z^{2}\right)^{5/2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial y}\left(x,y,z\right)=-y\left(x^{2}+y^{2}+z^{2}\right)^{-3/2}\rightarrow\frac{\partial^{2}f}{\partial y^{2}}\left(x,y,z\right)=\frac{2y^{2}-x^{2}-z^{2}}{\left(x^{2}+y^{2}+z^{2}\right)^{5/2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial z}\left(x,y,z\right)=-z\left(x^{2}+y^{2}+z^{2}\right)\rightarrow\frac{\partial^{2}f}{\partial z^{2}}\left(x,y,z\right)=\frac{2z^{2}-x^{2}-y^{2}}{\left(x^{2}+y^{2}+z^{2}\right)^{5/2}}
\]

\end_inset


\end_layout

