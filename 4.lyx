\begin_layout Subsection
Desarrollar la función 
\begin_inset Formula $p\left(x\right)=x^{4}-5x^{3}+5x^{2}+x+2$
\end_inset

 en potencias de 
\begin_inset Formula $x-2$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Calculo el polinomio de taylor en el punto }x=2
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{2}\left(x\right)=p(2)+p'(2)*(x-2)+\frac{p''(2)*\left(x-2\right)^{2}}{2}+\frac{p'''\left(2\right)\left(x-2\right)^{3}}{6}+\frac{p''''\left(2\right)\left(x-2\right)^{4}}{24}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{2}\left(x\right)=0+\left(3x^{3}-15x^{2}+10x+1\right)\bigg|_{x=2}*(x-2)+\frac{\left(9x^{2}-30x+10\right)}{2}\bigg|_{x=2}*\left(x-2\right)^{2}+\frac{\left(18x-30\right)}{6}\bigg|_{x=2}\left(x-2\right)^{3}+\frac{18}{24}\left(x-2\right)^{4}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{2}\left(x\right)=-15\left(x-2\right)-7\left(x-2\right)^{2}+\left(x-2\right)^{3}+\frac{3}{4}\left(x-2\right)^{4}=\left(x-2\right)\left(-15-7\left(x-2\right)+\left(x-2\right)^{2}+\frac{3}{4}\left(x-2\right)^{3}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Desarrollar la función 
\begin_inset Formula $g\left(x\right)=\sqrt{x}$
\end_inset

 en potencias de 
\begin_inset Formula $x-1$
\end_inset

 hasta orden 3.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{1}(x)=1+g'\left(1\right)\left(x-1\right)+\frac{g''\left(1\right)\left(x-1\right)}{2}+\frac{g'''\left(1\right)\left(x-1\right)}{6}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{1}(x)=1+\left(\frac{1}{2}x^{-1/2}\bigg|_{x=1}\right)\left(x-1\right)+\left(\frac{-\frac{1}{4}x^{-3/2}\bigg|_{x=1}}{2}\right)\left(x-1\right)+\left(\frac{\frac{3}{8}x^{-5/2}\bigg|_{x=1}}{2}\right)\left(x-1\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{1}(x)=1+\frac{1}{2}\left(x-1\right)+-\frac{1}{8}\left(x-1\right)+\frac{3}{16}\left(x-1\right)\sim g(x)
\]

\end_inset


\end_layout

\begin_layout Subsection
Hallar el polinomio de Maclaurin de grado tres para la función 
\begin_inset Formula $f\left(x\right)=\ln\left(x+1\right)^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{0}(x)=0+\frac{2\ln\left(x+1\right)}{x+1}\biggg|_{x=0}x+\left(-\frac{2\left(\ln\left(x+1\right)-1\right)}{\left(x+1\right)^{2}}\biggg|_{x=0}\right)\frac{x^{2}}{2}+\left(\frac{4\left(\ln\left(x+1\right)-1\right)-2}{\left(x+1\right)^{3}}\biggg|_{x=0}\right)\frac{x^{3}}{6}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{0}(x)=0+x^{2}+x^{3}
\]

\end_inset


\end_layout

\begin_layout Subsection
Hallar el polinomio de Maclaurin de grado tres para la función 
\begin_inset Formula $g\left(x\right)=e^{x+2}$
\end_inset


\end_layout

\begin_layout Standard
