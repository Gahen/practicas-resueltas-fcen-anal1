\begin_layout Subsection
\begin_inset Formula $f(x,y)=x^{3}y+e^{xy^{2}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Es un polinomio, por lo que es }C^{\infty}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial x}(x,y)=3x^{2}y+y^{2}e^{xy^{2}};\frac{\partial f}{\partial y}(x,y)=x^{3}+2xye^{xy^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x^{2}}(x,y)=6xy+y^{4}e^{xy^{2}};\frac{\partial^{2}f}{\partial y^{2}}(x,y)=4x^{2}y^{2}e^{xy^{2}}+2xe^{xy^{2}};
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial y\partial x}(x,y)=3x^{2}+2xy^{3}e^{xy^{2}}+2ye^{xy^{2}}\frac{\partial^{2}f}{\partial x\partial y}(x,y)=2xye^{xy^{2}}=3x^{2}+2ye^{xy^{2}}+2xy^{3}e^{xy^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Como era esperado }\frac{\partial^{2}f}{\partial y\partial x}(x,y)=\frac{\partial^{2}f}{\partial x\partial y}(x,y)
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x,y,z)=ye^{z}+\frac{e^{y}}{x}+xy\sin z$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial x}(x,y,z)=y\sin z-\frac{e^{y}}{x^{2}};\frac{\partial f}{\partial y}(x,y,z)=e^{Z}+\frac{e^{y}}{x}+x\sin z;\frac{\partial f}{\partial z}(x,y,z)=ye^{z}+xy\cos z
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x^{2}}(x,y,z)=\frac{2e^{y}}{x^{3}};\frac{\partial^{2}f}{\partial y^{2}}(x,y,z)=\frac{e^{y}}{x};\frac{\partial^{2}f}{\partial z^{2}}(x,y,z)=ye^{z}-xy\sin z
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x\partial y}(x,y)=-\frac{e^{y}}{x^{2}}+\sin z;\frac{\partial^{2}f}{\partial y\partial x}(x,y)=\sin z-\frac{e^{y}}{x^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial z\partial y}(x,y)=e^{Z}+x\cos z;\frac{\partial^{2}f}{\partial y\partial z}(x,y)=e^{z}+x\cos z
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x\partial z}(x,y)=y\cos z;\frac{\partial^{2}f}{\partial z\partial x}(x,y)=y\cos z
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x,y,z)=\sqrt{x^{2}+y^{2}}+\ln z$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial f}{\partial x}(x,y,z)=\frac{x}{\sqrt{x^{2}+y^{2}}};\frac{\partial f}{\partial y}(x,y,z)=\frac{y}{\sqrt{x^{2}+y^{2}}};\frac{\partial f}{\partial z}(x,y,z)=\frac{1}{z}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x^{2}}(x,y,z)=\frac{y^{2}}{\left(\sqrt{x^{2}+y^{2}}\right)^{3/2}};\frac{\partial^{2}f}{\partial y^{2}}(x,y,z)=\frac{x^{2}}{\left(\sqrt{x^{2}+y^{2}}\right)^{3/2}};\frac{\partial^{2}f}{\partial z^{2}}(x,y,z)=-\frac{1}{z^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x\partial y}(x,y)=\frac{\partial^{2}f}{\partial y\partial x}(x,y)=\frac{xy}{\left(\sqrt{x^{2}+y^{2}}\right)^{3/2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial z\partial y}(x,y)=0;\frac{\partial^{2}f}{\partial y\partial z}(x,y)=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial^{2}f}{\partial x\partial z}(x,y)=0;\frac{\partial^{2}f}{\partial z\partial x}(x,y)=0
\]

\end_inset


\end_layout

