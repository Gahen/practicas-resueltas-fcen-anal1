\begin_layout Subsection
\begin_inset Formula $\frac{1}{\sqrt{x}}\in\left(0,\frac{1}{2}\right)\text{?}$
\end_inset


\end_layout

\begin_layout Standard
Ninguna distancia bastará, ya que 
\begin_inset Formula $\frac{1}{\sqrt{16}}\notin\left(0,\frac{1}{2}\right)\Rightarrow\mbox{ cualquier distancia de 16 tendrá puntos donde }\frac{1}{\sqrt{x}}\notin\left(0,\frac{1}{2}\right)$
\end_inset


\end_layout

\begin_layout Subsection

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $\frac{1}{\sqrt{x}}\in\left(\frac{1}{4}-\frac{1}{10},\frac{1}{4}+\frac{1}{10}\right)\text{?}$
\end_inset


\end_layout

\begin_layout Standard
Como la función es continua 
\begin_inset Formula $\forall x\neq0$
\end_inset

 sabemos que 
\begin_inset Formula $\lim_{x\to16}\frac{1}{\sqrt{x}}=\frac{1}{\sqrt{16}}=\frac{1}{4}\Rightarrow\forall\varepsilon>0\exists\delta>0/\left|\frac{1}{\sqrt{x}}-\frac{1}{4}\right|<\varepsilon(\delta)\wedge|x-16|<\delta$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\varepsilon=\frac{1}{10}\Rightarrow\left|\frac{1}{\sqrt{x}}-\frac{1}{4}\right|<\frac{1}{10}\Leftrightarrow\begin{cases}
\frac{1}{\sqrt{x}}>\frac{1}{4}\Leftrightarrow x<4^{2}\Rightarrow & \frac{1}{\sqrt{x}}-\frac{1}{4}<\frac{1}{10}\Leftrightarrow\frac{1}{\sqrt{x}}<\frac{7}{20}\Leftrightarrow x>\frac{400}{49}\\
\frac{1}{\sqrt{x}}<\frac{1}{4}\Leftrightarrow x>4^{2}\Rightarrow & \frac{1}{\sqrt{x}}-\frac{1}{4}>-\frac{1}{10}\Leftrightarrow\frac{1}{\sqrt{x}}>\frac{3}{20}\Leftrightarrow x<\frac{400}{9}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\therefore x\in\left(\frac{400}{49},\frac{400}{9}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection

\family roman
\series medium
\shape up
\size normal
\emph off
